<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 设置背景色，可以根据需要更改或移除 -->
    <solid android:color="#FFFFFF"/>
    <!-- 设置边框颜色和宽度 -->
    <stroke
        android:width="1dp"
        android:color="#000000"/>
    <!-- 设置内边距，如果需要的话 -->
    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp"/>
    <!-- 设置边角圆滑度，如果需要的话 -->
    <corners android:radius="0dp"/>
</shape>